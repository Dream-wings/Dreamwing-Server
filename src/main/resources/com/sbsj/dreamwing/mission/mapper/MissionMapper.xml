<mapper namespace="com.sbsj.dreamwing.mission.mapper.MissionMapper">
    <select id="getQuiz" resultType="com.sbsj.dreamwing.mission.domain.QuizVO">
        SELECT QUIZ_ID,
               QUESTION,
               ANSWER,
               CHOICE_1,
               CHOICE_2,
               CHOICE_3,
               CHOICE_4
        FROM   QUIZ
        WHERE  QUIZ_ID = #{quizId}
    </select>
</mapper>