<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sbsj.dreamwing.volunteer.mapper.VolunteerDAO">


    <select id="getVolunteerList" parameterType="long" resultType="com.sbsj.dreamwing.volunteer.dto.VolunteerDTO">
        SELECT * FROM "VOLUNTEER" WHERE "VOLUNTEER_ID" = #{volunteerId}
    </select>

    <select id="getVolunteerDetail" parameterType="long" resultType="com.sbsj.dreamwing.volunteer.dto.VolunteerDTO">
        SELECT * FROM "VOLUNTEER" WHERE "VOLUNTEER_ID" = #{volunteerId}
    </select>


</mapper>
